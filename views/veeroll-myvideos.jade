                      span.icon-bar
                      span.icon-bar
                      span.icon-bar
                // END Left navbar
              .content-body
                table.table.table-bordered#user_videos
                  thead
                    tr
                      th Order No.
                      th Date
                      th Title
                      th Status
                      th Actions
                  tbody
                    each value, i in videos
                      tr
                        td #{value.id}
                        td #{value.date}
                        td
                          a.text-info(href='#') #{value.name}
                        td #{value.status}
                        td
                          a.btn.btn-xs.btn-success(href='/member/new-channel') Add New Channel
                          a.btn.btn-xs.btn-primary.dropdown-toggle(href='javascript:void(0);',data-toggle='modal', data-target='#publish_modal', title='Publish via Youtube') Upload To
                          a.btn.btn-xs.btn-info(href='#') Edit
                          a.btn.btn-xs.red.delet-users-video(href='javascript:void(0);' data-id='#{value.id}') Delete
                  tfoot
                    tr
                      th Order No.
                      th Date
                      th Title
                      th Status
                      th Actions        
