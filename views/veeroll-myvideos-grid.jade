script(src='/youtube/vendor/datatables-colvis/js/dataTables.colVis.js')
script(src='/youtube/vendor/datatables/media/js/dataTables.bootstrap.js')
script(type='text/javascript', src='/youtube/vendor/bootstrap-select/bootstrap-select.min.js')
script(type='text/javascript', src='/youtube/vendor/jquery-multi-select/jquery.multi-select.js')
script(src='/youtube/vendor/jquery-classyloader/js/jquery.classyloader.min.js')
