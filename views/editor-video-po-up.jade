#example___video.modal.modal-theme.fade(tabindex='-1', role='dialog')
    .modal-dialog.modal-custom
        .modal-content
            .modal-header
                button.close(type='button', data-dismiss='modal', aria-label='Close')
                    span(aria-hidden='true') &times;
                h4.modal-title EXAMPLE VIDEO
            .modal-body
                video(id="editor_video"  ,controls width="100%", height="100%", poster='', preload="none")
                    source(src="/templates#{video}" type='video/mp4')
            .modal-footer